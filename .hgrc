[ui]
merge = vimdiff

[merge-tools]
vimdiff.executable = vimdiff
vimdiff.args = -f -d $output -M $local $base $other -c "wincmd J" -c "set modifiable" -c "set write"
vimdiff.premerge = keep

[extensions]
extdiff =

[extdiff]
vimdiff =

[templatealias]
google_compact_desc_text = '{GOOG_prefix(GOOG_trim_desc(desc), termwidth-graphwidth)}'
